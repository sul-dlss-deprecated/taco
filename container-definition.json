{
  "containerDefinitions": [
    {
      "name": "taco",
      "image": "suldlss/taco:REPLACE_TAG",
      "essential": true,
      "memory": 150,
      "cpu": 10,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 0
        }
      ],
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "REPLACE_REGION"
        },
        {
          "name": "AWS_DISABLE_SSL",
          "value": "false"
        },
        {
          "name": "DYNAMO_DB_ENDPOINT",
          "value": "dynamodb.REPLACE_REGION.amazonaws.com"
        },
        {
          "name": "RESOURCE_TABLE_NAME",
          "value": "resources-REPLACE_ENVIRONMENT"
        },
        {
          "name": "KINESIS_ENDPOINT",
          "value": "kinesis.REPLACE_REGION.amazonaws.com"
        },
        {
          "name": "DEPOSIT_STREAM_NAME",
          "value": "deposit-REPLACE_ENVIRONMENT"
        },
        {
          "name": "S3_ENDPOINT",
          "value": "s3.REPLACE_REGION.amazonaws.com"
        },
        {
          "name": "S3_BUCKET_NAME",
          "value": "taco-deposited-files-REPLACE_ENVIRONMENT"
        }
      ]
    }
  ],
  "family": "taco",
  "taskRoleArn": "arn:aws:iam::REPLACE_ACCOUNT_ID:role/REPLACE_ENVIRONMENT_ecs_task_role"
}
